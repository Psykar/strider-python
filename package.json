{
  "name": "strider-python",
  "version": "0.2.0",
  "description": "Python/Pyramid/Django support for Strider Continuous Deployment",
  "scripts": {
    "test": "jshint && mocha -R tap"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/Strider-CD/strider-python.git"
  },
  "keywords": [
    "strider",
    "python"
  ],
  "strider": {
    "type": "job",
    "id": "python",
    "title": "Python",
    "webapp": "webapp.js",
    "worker": "worker.js",
    "icon": "icon.png",
    "config": true
  },
  "author": "Niall O'Higgins",
  "license": "BSD",
  "dependencies": {
    "strider-detection-rules": "0.0.1"
  },
  "devDependencies": {
    "chai": "~1.8.1",
    "jshint": "~2.3.0",
    "mocha": "~1.14.0"
  }
}
